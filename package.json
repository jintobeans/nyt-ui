{
    "name": "nyt-ui",
    "version": "1.0.0",
    "description": "A user interface for browsing NYT Bestsellers",
    "engines": {
        "node": ">= 7.0.0"
    },
    "main": "index.js",
    "scripts": {
        "build-client": "webpack & sass public/sass:public",
        "build-client-watch": "webpack -w & sass --watch public/sass:public",
        "deploy": "script/deploy",
        "lint": "eslint ./ --ignore-path .gitignore",
        "postinstall": "touch secrets.js",
        "start": "node server",
        "start-dev": "NODE_ENV='development' npm run build-client-watch & npm run start-server",
        "start-server": "nodemon server -e html,js,scss --ignore public",
        "dans-test": "mocha ./server/**/*.spec.js ./server/**/**/*.spec.js ./client/**/*.spec.js --compilers js:babel-register",
        "test": "NODE_ENV='test' DATABASE_URL='postgres://localhost:5432/capstone-test' mocha -w ./server/**/*.spec.js ./server/**/**/*.spec.js ./client/**/*.spec.js --compilers js:babel-register --require babel-polyfill"
    },
    "author": "JT Wang",
    "repository": {
        "type": "git",
        "url": "https://github.com/jintobeans/nyt-ui.git"
    },
    "license": "ISC",
    "dependencies": {
        "axios": "^0.15.3",
        "babel-plugin-transform-class-properties": "^6.24.1",
        "babel-plugin-transform-runtime": "^6.23.0",
        "babel-polyfill": "^6.26.0",
        "babel-preset-es2015": "^6.24.1",
        "babel-preset-stage-3": "^6.24.1",
        "body-parser": "^1.16.1",
        "compression": "^1.7.1",
        "cors": "^2.8.4",
        "express": "^4.14.1",
        "fetch-jsonp": "^1.1.3",
        "history": "^4.6.3",
        "jsonp": "^0.2.1",
        "morgan": "^1.8.1",
        "prop-types": "^15.5.8",
        "react": "^16.0.0",
        "react-dom": "^16.0.0",
        "react-redux": "^5.0.2",
        "react-router-dom": "^4.1.1",
        "redux": "^3.6.0",
        "redux-logger": "^2.8.1",
        "redux-thunk": "^2.2.0",
        "webfontloader": "^1.6.28"
    },
    "devDependencies": {
        "axios-mock-adapter": "^1.9.0",
        "babel-core": "^6.22.1",
        "babel-eslint": "^8.0.1",
        "babel-loader": "^6.2.10",
        "babel-plugin-recharts": "^1.1.1",
        "babel-preset-env": "^1.6.1",
        "babel-preset-react": "^6.22.0",
        "babel-preset-stage-2": "^6.24.1",
        "babel-register": "^6.24.1",
        "chai": "^3.5.0",
        "enzyme": "^3.0.0",
        "enzyme-adapter-react-16": "^1.0.0",
        "eslint": "^4.10.0",
        "eslint-config-fullstack": "^3.0.0",
        "eslint-plugin-react": "^7.4.0",
        "json-loader": "^0.5.7",
        "mocha": "^3.5.3",
        "nodemon": "^1.12.1",
        "raw-loader": "^0.5.1",
        "react-test-renderer": "^16.0.0",
        "redux-devtools-extension": "^2.13.2",
        "redux-mock-store": "^1.2.3",
        "supertest": "^3.0.0",
        "webpack": "^2.2.1",
        "webpack-livereload-plugin": "^1.0.0"
    }
}
